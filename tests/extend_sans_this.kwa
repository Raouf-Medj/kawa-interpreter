
var paire p;
var paire pt;
var triple t;

class paire {
  attribute int x;
  attribute int y;

  method int ok() {
    return 5;
  }
}

class triple extends paire {
  attribute int z;

  method int ok() {
    return 10;
  }
}

main {
  p = new paire;
  t = new triple;
  p.x = 1;
  p.y = 2;
  t.x = 3;
  t.y = 4;
  t.z = 5;
  print(p.ok());
  print(t.ok());
  print(p.x + p.y);
  print(t.x + t.y + t.z);
  print(t.x * t.y * t.z);
  pt = new triple;
  pt.x = 1;
  pt.y = 2;
  pt.z = 3;
  print(pt.x + pt.y + pt.z);
  // affiche 3, puis 12, puis 60, puis 6.
}
